export struct DeckNode {
	id: duration,
	name: string,
	level: int,
	collapsed: bool,
	new: int,
	learn: int,
	due: int,
	has_children: bool,
	parent_index: int,
}

export struct DeckTree {
	due_total: int,
	deck_nodes: [DeckNode],
}

export struct CardNode {
	id: duration,
	deck: DeckNode,
	question: string,
	answer: string,
	durations: [string],
}
